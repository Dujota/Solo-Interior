// "next/core-web-vitals",
{
  "extends": [
    "wesbos",
    "plugin:import/errors"
  ],
  "rules": {
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "alias": {
        "extensions": [
          ".js",
          ".jsx"
        ],
        "map": [
          [
            "@",
            "."
          ]
        ]
      }
    }
  }
}